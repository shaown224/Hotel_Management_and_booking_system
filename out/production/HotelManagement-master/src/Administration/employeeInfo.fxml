<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #FF5733;" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Administration.EmployeeInfo">
    <children>
        <AnchorPane prefHeight="485.0" prefWidth="733.0" style="-fx-background-color: #372a78;">
            <children>
                <TableView fx:id="tableview" layoutX="94.0" layoutY="1.0" prefHeight="485.0" prefWidth="639.0">
                    <columns>
                        <TableColumn fx:id="colName" prefWidth="75.0" text="Name" />
                        <TableColumn fx:id="colAge" prefWidth="75.0" text="Age" />
                        <TableColumn fx:id="colGender" prefWidth="75.0" text="Gender" />
                        <TableColumn fx:id="colJob" prefWidth="75.0" text="Job" />
                        <TableColumn fx:id="colSalary" prefWidth="75.0" text="Salary" />
                        <TableColumn fx:id="colPhone" prefWidth="87.0" text="Phone" />
                        <TableColumn fx:id="colEmail" prefWidth="162.0" text="Email" />
                    </columns>
                </TableView>
                <Button fx:id="loadData" layoutX="7.0" layoutY="195.0" mnemonicParsing="false" onAction="#funcLoadData" text="Load Data" textFill="#1b1717">
                    <font>
                        <Font name="System Bold" size="12.0" />
                    </font>
                </Button>
                <Button fx:id="back" layoutX="23.0" layoutY="243.0" mnemonicParsing="false" onAction="#funcBack" text="Back">
                    <font>
                        <Font name="System Bold" size="12.0" />
                    </font>
                </Button>
            </children>
        </AnchorPane>
    </children>
</AnchorPane>
