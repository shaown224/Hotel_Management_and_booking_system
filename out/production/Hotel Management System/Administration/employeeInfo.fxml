<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #FF5733;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Administration.EmployeeInfo">
    <children>
        <AnchorPane layoutX="4.0" layoutY="5.0" prefHeight="480.0" prefWidth="729.0" style="-fx-background-color: #FF5733;">
            <children>
                <TableView fx:id="tableview" layoutX="51.0" layoutY="63.0" prefHeight="294.0" prefWidth="627.0">
                    <columns>
                        <TableColumn fx:id="colName" prefWidth="75.0" text="Name" />
                        <TableColumn fx:id="colAge" prefWidth="75.0" text="Age" />
                        <TableColumn fx:id="colGender" prefWidth="75.0" text="Gender" />
                        <TableColumn fx:id="colJob" prefWidth="75.0" text="Job" />
                        <TableColumn fx:id="colSalary" prefWidth="75.0" text="Salary" />
                        <TableColumn fx:id="colPhone" prefWidth="87.0" text="Phone" />
                        <TableColumn fx:id="colEmail" prefWidth="162.0" text="Email" />
                    </columns>
                </TableView>
                <Button fx:id="loadData" layoutX="280.0" layoutY="404.0" mnemonicParsing="false" onAction="#funcLoadData" text="Load Data">
                    <font>
                        <Font name="System Bold" size="12.0" />
                    </font>
                </Button>
                <Button fx:id="back" layoutX="414.0" layoutY="403.0" mnemonicParsing="false" onAction="#funcBack" text="Back">
                    <font>
                        <Font name="System Bold" size="12.0" />
                    </font>
                </Button>
            </children>
        </AnchorPane>
    </children>
</AnchorPane>
